      <sensor name="rp_lidar" type="ray">
        <always_on>true</always_on>
        <visualize>true</visualize>
        <pose>0.0 0 0.02 0 0 0</pose>
        <update_rate>5.5</update_rate> <!-- RPLidar A1 is 5.5 Hz -->
        <ray>
