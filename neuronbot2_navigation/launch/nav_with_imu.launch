<launch>

  <include file="$(find neuronbot2_bringup)/launch/robot_with_imu.launch"/>

  <param name="use_sim_time" value="false" />

  <arg name="map_name" default="office_map.yaml" />

  <node name="map_server" pkg="map_server" type="map_server" args="$(find neuronbot2_navigation)/maps/$(arg map_name)"/>

  <include file="$(find neuronbot2_navigation)/launch/include/move_base.launch.xml" />

  <include file="$(find neuronbot2_navigation)/launch/include/amcl.launch.xml" />
  
</launch>
